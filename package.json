{
  "name": "example-sourcemap",
  "private": true,
  "scripts": {
    "build": "esbuild --bundle --minify --sourcemap --platform=node --target=node18 --outfile=./dist/index.js ./src/index.ts",
    "parse": "esbuild --bundle --platform=node --target=node18  ./parse.ts | node",
    "serve": "esbuild --bundle --platform=node --target=node18  ./serve.ts | node"
  },
  "author": "mryhryki",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "esbuild": "^0.17.18",
    "express": "^4.18.2",
    "vlq": "^2.0.4"
  }
}
